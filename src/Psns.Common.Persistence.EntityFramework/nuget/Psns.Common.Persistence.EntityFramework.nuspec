<?xml version="1.0"?>
<package >
  <metadata>
    <id>Psns.Common.Persistence.EntityFramework</id>
    <title>PSNS Common Entity Framework Library</title>
    <authors>Jeff Purcell</authors>
    <owners>109.14</owners>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <description>Contains a library that implements Psns.Common.Persistence.Definitions with Entity Framework</description>
    <projectUrl>https://github.com/PSNS-IMF/persistence-entityframework</projectUrl>
    <licenseUrl>https://github.com/PSNS-IMF/persistence-entityframework/blob/master/LICENSE.md</licenseUrl>
